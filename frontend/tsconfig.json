{
    "compilerOptions": {
      "target": "es5", 
      "lib": ["dom", "es2015"],
      "allowJs": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  